{
  "pull_request": {
    "tasks": [
      {
        "executor": "jenkins",
        "context": "functional",
        "options": {
          "tags": [
            "@smoke"
          ],
          "browsers": [
            "chrome"
          ]
        }
      },
      {
        "executor": "TeamCity",
        "context": "projectName",
        "options": {
          "build_tag" : "proj_wasX",
          "build_env" : "env_dev",
          "cuke_env" : "env_dev|staging|prod",
          "cuke_tags" : [
            "@proj_wasX",
            "@proj_wasn"
          ]
        }
      },
      {
        "executor": "travis",
        "context": "integration",
        "options": {
          "tags": [
            "@a11y"
          ]
        }
      }
    ]
  }
}
